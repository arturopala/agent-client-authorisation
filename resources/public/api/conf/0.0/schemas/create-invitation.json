{
  "type": "object",
  "description": "Creates a new invitation between the agency and client",
  "properties": {
    "service": {
      "$ref": "service.json"
    },
    "clientIdType": {
      "type": "string",
      "description" : "The type of identifier that clientId is. Currently must be \"ni\""
    },
    "clientId": {
      "type": "string",
      "description" : "Client identifier (of type clientIdType)"
    },
    "clientPostcode": {
      "type": "string",
